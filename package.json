{
  "name": "furnish",
  "version": "0.1.0",
  "description": "Real-time Configuration Managment with Node.js",
  "author": {
    "name": "justmiles"
  },
  "homepage": "https://github.com/justmiles/furnish",
  "bugs": {
    "url": "https://github.com/justmiles/furnish/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/justmiles/furnish.git"
  },
  "bin": "bin/furnish.js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "./node_modules/codo/bin/codo -o docs src README.md; ./node_modules/docco/bin/docco examples/* -o docs/examples",
    "postinstall" : "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\"",
    "build": "grunt build"
  },
  "license": "GNU GENERAL PUBLIC LICENSE",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "coffee-script": "^1.10.0",
    "commander": "^2.9.0",
    "decompress": "^3.0.0",
    "ejs": "^2.4.1",
    "eventemitter2": "^2.2.0",
    "mkdirp": "^0.5.1",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "codo": "^2.1.2",
    "docco": "^0.7.0",
    "grunt": "^1.0.1",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-nexe": "^0.1.0"
  },
  "keywords": [
    "furnish",
    "furnish-cli",
    "configuration",
    "managment",
    "devops"
  ],
  "engines": {
    "node": ">=4.x"
  },
  "nexe": {
    "input": "./index.js",
    "output": "furnish^$",
    "temp": "build",
    "browserify": {
      "requires": [],
      "excludes": [],
      "paths": []
    },
    "runtime": {
      "framework": "node",
      "version": "4.2.2",
      "js-flags": "--use_strict",
      "ignoreFlags": true
    }
  }
}
